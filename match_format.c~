#include "main.h"
#include <stdarg.h>
#include <stddef.h>


int (*match_format(const char *type))(va_list)
{
	int i = 0;

	print_type s[] = {
		{"s", print_string},
		{"c", print_char},
		{NULL, NULL}
	};

	for (; s[i].type != NULL; i++)
	{
		if (*(s[i].type) == *type)
			break;
	}
	return (s[i].func);
}
